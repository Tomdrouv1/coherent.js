{
  "name": "@coherent.js/fastify",
  "version": "1.0.0-beta.1",
  "description": "Fastify adapter for Coherent.js",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "../../src/fastify/coherent-fastify.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "node build.mjs",
    "clean": "rm -rf dist/",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "test:coverage": "vitest run --coverage",
    "test:vitest": "vitest run",
    "test:node": "node --test test/*.test.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tomdrouv1/coherent.js.git"
  },
  "peerDependencies": {
    "fastify": ">=4.0.0 < 6.0.0",
    "@coherent.js/core": "1.0.0-beta.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
